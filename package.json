{
  "name": "unplugin-react-markdown",
  "type": "module",
  "version": "0.1.9",
  "packageManager": "pnpm@8.6.12",
  "description": "Compile Markdown to React component",
  "license": "MIT",
  "homepage": "https://github.com/clovu/unplugin-react-markdown#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clovu/unplugin-react-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/clovu/unplugin-react-markdown/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "markdown-it"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "markdown-it": "^14.1.0",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "gray-matter": "^4.0.3",
    "jsxify-html": "^1.0.3",
    "unplugin": "^1.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.3.1",
    "@nuxt/kit": "^3.13.1",
    "@nuxt/schema": "^3.13.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.5.4",
    "bumpp": "^9.5.2",
    "chalk": "^5.3.0",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "markdown-it": "^14.1.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^2.0.5",
    "webpack": "^5.94.0"
  }
}
